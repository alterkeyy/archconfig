    "group/cpu_temp": {
        "orientation": "inherit",
        "drawer": {
            "transition-duration": 500,
            "children-class": "not-power",
            "transition-left-to-right": true
        },
        "modules": [
            "cpu",
            "temperature"
        ]
    },
    // CPU
    "cpu": {
        "format": "ï‹›  {usage}% ",
        "interval": 5
        // "tooltip-format":"{avg_frequency}"//this is uesless
        // "on-click": "~/.config/ml4w/settings/system-monitor.sh"
    },
    //temp
    "temperature": {
        "thermal-zone": 1,
        "format": "{temperatureC}Â°C ï‹‰",
        "critical-threshold": 80,
        "format-critical": "{temperatureC}Â°C ï‹‡"
    },
    //gpu
    "custom/nvidia": {
        "exec": "nvidia-smi --query-gpu=utilization.gpu --format=csv,nounits,noheader | sed 's/\\([0-9]\\+\\)/ \\1%/g'",
        "format": "G {}",
        "interval": 2
    },
    // Memory
    "memory": {
        "interval": 5,
        "format": "M {}% "
        // "on-click": "~/.config/ml4w/settings/system-monitor.sh"
    },
    // Harddisc space used
    "disk": {
        "interval": 10,
        "format": "ïƒ‡  {percentage_used}% ",
        "path": "/"
        // "on-click": "~/.config/mddl4w/settings/system-monitor.sh"
    },
    "group/speed": {
        "orientation": "inherit",
        "drawer": {
            "transition-duration": 500,
            "children-class": "not-power",
            "transition-left-to-right": true
        },
        "modules": [
            "network#speeddown",
            "network#speedup"
        ]
    },
